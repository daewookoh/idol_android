<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="EditTexttiber" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/tiber_edit_text_holo_light</item>
	  <item name="android:textColor">@color/gray1000</item>
  </style>

  <style name="AutoCompleteTextViewtiber" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/tiber_list_selector_holo_light</item>
	  <item name="android:background">@drawable/tiber_edit_text_holo_light</item>
	  <item name="android:textColor">@color/gray1000</item>
  </style>

  <style name="CheckBoxtiber" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/tiber_btn_check_holo_light</item>
  </style>

  <style name="Buttontiber" parent="android:Widget.Button">
	  <item name="android:background">@drawable/btn</item>
	  <item name="android:minHeight">40dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">@color/text_default</item>
	  <item name="android:textSize">14.5sp</item>
  </style>

  <style name="ImageButtontiber" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/tiber_btn_default_holo_light</item>
  </style>

  <style name="Spinnertiber" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/tiber_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/tiber_list_selector_holo_light</item>
  </style>

  <style name="Tabtiber">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">1dip</item>
      <item name="android:paddingRight">1dip</item>
      <item name="android:background">@drawable/tiber_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">0dip</item>
  </style>

  <style name="TabTexttiber">
	  <item name="android:textColor">@color/gray1000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">0dip</item>
  </style>

  <style name="ProgressBartiber" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/tiber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/tiber_progress_indeterminate_horizontal_holo_light</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBartiber" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/tiber_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/tiber_scrubber_progress_horizontal_holo_light</item>
      <item name="android:minHeight">13dip</item>
      <item name="android:maxHeight">13dip</item>
      <item name="android:thumb">@drawable/tiber_scrubber_control_selector_holo_light</item>
      <item name="android:thumbOffset">16dip</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
  </style>

  <style name="Toggletiber" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/tiber_btn_toggle_holo_light</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">@color/gray1000</item>
  </style>

  <style name="ListViewtiber" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/tiber_list_selector_holo_light</item>
  </style>

  <style name="ListViewtiber.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/tiber_list_selector_holo_light</item>
  </style>

  <style name="SpinnerItemtiber" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">@color/gray1000</item>
  </style>

</resources>
